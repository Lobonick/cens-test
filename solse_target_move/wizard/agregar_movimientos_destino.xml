<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="agregar_asiento_destino_wizard" model="ir.ui.view">
        <field name="name">saccount.generar.asientos.destino.form</field>
        <field name="model">saccount.generar.asientos.destino</field>
        <field name="arch" type="xml">
        <form>
            <h1>
                Generar Asientos Destino
            </h1>
            <group>
                <group>
                    <field name="fecha_ini" />
                    <field name="fecha_fin" />
                </group>
            </group>
            <footer>
                <button name="crear_movimientos" string="Generar Asientos" type="object" default_focus="1" class="oe_highlight"/>
                <button string="Cancel" class="btn btn-secondary" special="cancel" />
            </footer>
        </form>
        </field>
    </record>
</odoo>
